    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="blog" data-nav-color="red">
      <div class="my-auto">
        <h2 class="mb-5">
          <%= data.i18n.en.sections.blog.title %>
        </h2>
        <div class="row">
          <article class="col-sm-12 col-md-6">
            <% page_articles.sort_by{|a| a.date}.reverse.each_with_index do |article, i| %>
              <% if i == 0 %>
                  <header>
                    <img class="post-image" src="<%= article.data.thumbnail_image %>">
                    <h2 class="main-post-title">
                      <%= link_to article.title, article %>
                    </h2>
                    <span class="main-post-category">
                      <%= article.data.category %>
                    </span>
                    <span class="main-post-date">
                      <%= article.date.strftime('%b %e, %Y') %>
                    </span>
                  </header>
                <div class="post-body"><%= article.summary(300, '...') %></div>
              <% end %>
            <% end %>
          </article>
          <article class="col-sm-12 col-md-6">
            <% page_articles.sort_by{|a| a.date}.reverse.each_with_index do |article, i| %>
              <% if (i > 0) && (i < 3) %>
                <div class="main-blog-post">
                  <header>
                    <h2 class="post-title">
                      <%= link_to article.title, article %></h2>
                    <span class="main-post-category">
                      <%= article.data.category %> |
                    </span>
                    <span class="post-date">
                      <%= article.date.strftime('%b %e, %Y') %>
                    </span>
                  </header>
                  <div class="post-body"><%= article.summary(125, '...') %></div>
                </div>
              <% end %>
              <% if (i == 3) %>
                  <div class="main-blog-post__last">
                    <header>
                      <h2 class="post-title"><%= link_to article.title, article %></h2>
                      <span class="main-post-category">
                        <%= article.data.category %> |
                      </span>
                      <span class="post-date">
                        <%= article.date.strftime('%b %e, %Y') %>
                      </span>
                    </header>
                    <div class="post-body"><%= article.summary(125, '...') %></div>
                  </div>
              <% end %>
            <% end %>
          </article>
        </div>
        

        <div class="row">
          <div class="col-sm-12 btn-blog-block">
            <a href="/blog" target="_blank" class="btn btn-primary btn-main-blog">MORE POSTS</a>
          </div>
        </div>
      </div>
    </section>

